<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<Button mnemonicParsing="false" styleClass="petButton" stylesheets="@../css/petButton.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <graphic>
      <VBox styleClass="vBox" stylesheets="@../css/petButton.css">
         <children>
            <Pane styleClass="photo" stylesheets="@../css/petButton.css">
               <children>
                  <ImageView fitHeight="21.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </Pane>
            <HBox styleClass="hBoxExternal" stylesheets="@../css/petButton.css">
               <children>
                  <HBox styleClass="hBoxInternal" stylesheets="@../css/petButton.css">
                     <children>
                        <Label styleClass="name" stylesheets="@../css/petButton.css" text="Label" />
                        <Circle fill="DODGERBLUE" radius="2.0" stroke="BLACK" strokeType="INSIDE" styleClass="point" />
                        <Label styleClass="month" stylesheets="@../css/petButton.css" text="Label" />
                        <Pane styleClass="sex" stylesheets="@../css/petButton.css">
                           <children>
                              <ImageView fitHeight="18.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" />
                           </children>
                        </Pane>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </graphic>
</Button>
